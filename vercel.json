{
  "version": 2,
  "routes": [
    {
      "src": "/api/categorize",
      "dest": "/server.js"
    },
    {
      "src": "/src/main.js",
      "dest": "/src/main.js"
    },
    {
      "src": "/src/assets/css/main.css",
      "dest": "/src/assets/css/main.css"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}